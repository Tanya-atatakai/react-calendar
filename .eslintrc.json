{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "flowtype"
  ],
  "rules": {
    "import/extensions": "off", 
    "no-underscore-dangle": "off", 
    "import/prefer-default-export": "off", 
    "import/no-extraneous-dependencies": ["error"],
    "no-reserved-keys": false
  }
}